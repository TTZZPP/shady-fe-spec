{
  "name": "shady-fe-lint",
  "version": "0.0.8",
  "description": "前端编码规范工程化脚手架",
  "bin": "./lib/cli.js",
  "main": "./lib/index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run copyfiles && tsc -w",
    "build": "rd /s /q lib && npm run copyfiles && tsc",
    "copyfiles": "copyfiles -a -u 1 \"src/config/**\" lib",
    "test": "npm run build && node --experimental-vm-modules --trace-deprecation node_modules/jest/bin/jest.js",
    "coverage": "nyc jest --silent --forceExit",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "lib/"
  ],
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "keywords": [
    "encode",
    "lint",
    "cli"
  ],
  "devDependencies": {
    "@types/command-exists": "^1.2.3",
    "@types/cross-spawn": "^6.0.6",
    "@types/ejs": "^3.1.5",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@types/prettier": "^3.0.0",
    "@types/stylelint": "^14.0.0",
    "@types/text-table": "^0.2.5",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "nyc": "^17.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/preset-react": "^7.26.3",
    "@commitlint/cli": "^19.8.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "chalk": "^5.4.1",
    "command-exists": "^1.2.9",
    "commander": "^13.1.0",
    "cross-spawn": "^7.0.6",
    "ejs": "^3.1.10",
    "eslint": "^9.24.0",
    "eslint-config-egg": "^14.1.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vue": "^10.0.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "inquirer": "^12.5.2",
    "is-docker": "^3.0.0",
    "lodash": "^4.17.21",
    "markdownlint": "^0.37.4",
    "markdownlint-rule-helpers": "^0.28.0",
    "ora": "^8.2.0",
    "prettier": "^3.5.3",
    "shady-fe-commitlint-config": "workspace:^",
    "shady-fe-eslint-config": "workspace:^",
    "shady-fe-markdownlint-config": "workspace:^",
    "shady-fe-stylelint-config": "workspace:^",
    "strip-ansi": "^7.1.0",
    "stylelint": "^16.18.0",
    "stylelint-scss": "^6.11.1",
    "terminal-link": "^4.0.0",
    "text-table": "^0.2.0",
    "vue-eslint-parser": "^10.1.3"
  },
  "author": "TZP",
  "homepage": "https://github.com/TTZZPP/shady-fe-spec#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TTZZPP/shady-fe-spec.git"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/__tests__/*.test.js"
    ]
  }
}
